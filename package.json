{
  "name": "cleanarchts",
  "version": "1.0.0",
  "description": "Clean archetecture template implemented in TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "mocha -r ts-node/register ./src/**/*.spec.ts",
    "coverage": "nyc mocha -r ts-node/register ./test/**/*.spec.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/warrenbell/cleanarchTS.git"
  },
  "keywords": [
    "clean archetecture",
    "typescript"
  ],
  "author": "Warren Bell",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/warrenbell/cleanarchTS/issues"
  },
  "homepage": "https://github.com/warrenbell/cleanarchTS#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^5.2.2",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "source-map-support": "^0.5.6",
    "ts-node": "^6.1.1",
    "typescript": "^2.9.2"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "./src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "coverage/**/*",
      "dist/**/*",
      "node_modules/**/*",
      "test/**/*"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true
  }
}
